isbn = {9781450324748},
journal = {Conference on Human Factors in Computing Systems - Proceedings},
keywords = {Local music discovery,Personalized internet radio,Recommender systems},
pages = {2023--2028},
title = {{Using personalized radio to enhance local music discovery}},
year = {2014}
}
@article{Borja2015,
abstract = {Previous studies examining the intention and level of music piracy among young people have postulated that collective attitudes, optimistic biases toward risk, and beliefs about copyright laws are the key factors involved in their decisions. We extend the analysis by studying the impact of music streaming services. Music streaming is an alternative business model in which, for a small fee, the consumer has access to a large set of songs without downloading them onto their devices. Results from a Logit model show that college students who are frequent users of music streaming are also more likely to download music illegally. A plausible explanation is that those engaged in music streaming are also heavy users of computer technology, software downloading, and digital sharing - factors that facilitate the conditions for music piracy. Demographics, collective attitudes, and beliefs about risk and rewards continue to play a key role in explaining music piracy, but their relevance is slightly reduced once controlled for music streaming usage.},
author = {Borja, Karla and Dieringer, Suzanne and Daw, Jesse},
doi = {10.1016/j.chb.2014.11.088},
file = {:Users/miguelregouga/Documents/Papers Tese/borja2015.pdf:pdf},
issn = {07475632},
journal = {Computers in Human Behavior},
keywords = {Attitudes and collective views,Digital music piracy,Logit model,Music industry,Music streaming},
pages = {69--76},
publisher = {Elsevier Ltd},
title = {{The effect of music streaming services on music piracy among college students}},
url = {http://dx.doi.org/10.1016/j.chb.2014.11.088},
volume = {45},
year = {2015}
}
